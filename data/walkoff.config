{
    "auth":{
        "https":"true",
        "TLS_version":"1.2",
        "certificatePath":"auth/shortstop.public.pem",
        "privateKeyPath":"auth/shortstop.private.pem"
    },
    "execution":{
        "maxProcesses":4,
        "secondsDelay":1
    },
    "global":{
        "appsPath":"apps",
        "dbPath":"data/walkoff.db",
        "playbookPath":"data/playbook.json"
    },
    "interface":{
        "debug":"True",
        "defaultServer":"True",
        "host":"127.0.0.1",
        "port":"5000"
    },
    "logging":{
        "logPath":"data/logs/"
    }
}